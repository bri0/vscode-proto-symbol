{
    "name": "proto3-nav",
    "displayName": "proto3-nav",
    "description": "Navigation through proto3",
    "version": "0.1.1",
    "engines": {
        "vscode": "^1.33.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:proto3"
    ],
    "main": "./out/extension",
    "contributes": {
        "languages": [
            {
                "id": "proto3",
                "extensions": [
                    ".proto"
                ]
            }
        ]
    },
    "repository": {
        "url": "https://github.com/bri0/vscode-proto-symbol"
    },
    "publisher": "bri0",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.1.4",
        "vscode": "^1.1.25",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
